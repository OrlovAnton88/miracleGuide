    <div class="list-block contacts-list">
        <ul>
            {{#model.routes}}
                <a href="route.html?id={{name}}" class="item-link item-content">
                    <div class="item-media">
                        <i class="icon ion-ios7-person"></i>
                    </div>
                    <div class="item-inner">
                        <div class="item-title">{{name}} {{level}}</div>
                    </div>
                </a>
            {{/model.routes}}
        </ul>
    </div>
</div>